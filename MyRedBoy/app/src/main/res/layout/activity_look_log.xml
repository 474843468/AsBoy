    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_look_log"
    android:layout_width="match_parent"

    android:background="@drawable/select_good_bg"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/layout_more_titlebar"/>

    <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:id="@+id/tv_log"
          android:layout_marginTop="100dp"
           android:layout_gravity="center"
          android:text="没有找到浏览记录"
          android:textSize="23sp"
          android:textColor="#000"
          android:drawableBottom="@drawable/commodity_hua_validate_lable_two"/>

</LinearLayout>
